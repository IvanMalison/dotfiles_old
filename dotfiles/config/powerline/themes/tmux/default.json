{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "uptime": {
      "before": "⇑  "
    },
    "date": {
      "before": "⌚ "
    }
  },
  "segments": {
    "right": [
      {
	"function": "system_load",
	"priority": 50
      },
      {
	"function": "uptime",
	"priority": 50
      },
      {
	"function": "date",
	"args": {
	  "format": "%H:%M",
	  "istime": true
	}
      },
      {
	"function": "external_ip"
      },
      {
	"function": "hostname"
      }
    ],
    "special": [
      {
        "function": "window_status",
        "type": "string",
        "contnets": "#I"
      }
    ]
  }
}
